{
	"name": "Jayanth's Devil Dark Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#151a21",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#0544a8",
		"sideBarTitle.foreground": "#868a8f",
		"window.activeBorder":"#012b6e",
		"toolbar.hoverBackground":"#184ea3",
		"toolbar.activeBackground":"#2d323b",
		"button.background":"#06337a",
		"dropdown.background":"#2c3a66",
		"dropdown.border":"#012b6e",
		"statusBar.background":"#0d2673",
		"statusBar.border":"#012b6e",
		"titleBar.activeBackground":"#081526",
		"titleBar.inactiveBackground":"#273561",
		"menu.background":"#2c3a66",
		"menu.selectionBackground":"#0544a8",
		"breadcrumb.background":"#131e2e",
		"breadcrumb.focusForeground":"#ececec",
		"sideBar.background":"#142030",
		"sideBarSectionHeader.background":"#111c2b",
		"activityBar.background":"#0f1a29",
		"activityBar.dropBorder":"#13181f",
		"activityBar.border":"#13181f",
		"editorGroupHeader.tabsBackground":"#0f1a29",
		"tab.inactiveBackground":"#152336",
		"editorLineNumber.foreground":"#5f6369",
		"editorLineNumber.activeForeground":"#bec1c4"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#4487a7"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#84addb"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#b63147"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#e00975"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#c70660"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#4ce0be"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#077dcc"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#04bf8d"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#bbf00e"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
